.btn-circle span {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 42px;
  height: 4px;
  background: #fff;
}
.btn-circle span:before, .btn-circle span:after {
  content: "";
  background: #fff;
  height: 4px;
  position: absolute;
  left: 0;
}
.btn-circle span:before {
  width: 100%;
  top: -9px;
}
.btn-circle span:after {
  width: 60%;
  bottom: -9px;
